{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"main": "server.ts",
	"scripts": {
		"start:dev": "nodemon",
		"start": "ts-node -r tsconfig-paths/register --logError src/server.ts",
		"migrate": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
		"make:model": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize model:create --name",
		"seed": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
		"make:migration": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize migration:generate --name",
		"migrate:undo": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo",
		"seed:undo": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize db:seed:undo",
		"seed:make": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize seed:generate --name",
		"build": "tsc && npx tsc-alias",
		"start:prod": "node dist/server.js",
		"deploy": "npm run build && pm2 start npm --name 'Stock Management API' -- run start:prod"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"aws-sdk": "^2.1691.0",
		"axios": "^1.7.7",
		"bcrypt": "^5.1.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"cloudinary": "^2.4.0",
		"compression": "^1.7.4",
		"config": "^3.3.12",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"exceljs": "^4.4.0",
		"express": "^4.18.2",
		"helmet": "^7.1.0",
		"hpp": "^0.2.3",
		"ioredis": "^5.4.1",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"mysql2": "^3.7.0",
		"pg": "^8.12.0",
		"reflect-metadata": "^0.2.2",
		"sequelize": "^6.35.2",
		"sharp": "^0.33.5",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.0",
		"tslib": "^2.6.2",
		"uuid": "^10.0.0",
		"winston": "^3.11.0",
		"winston-daily-rotate-file": "^4.7.1",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/hpp": "^0.2.5",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/morgan": "^1.9.9",
		"@types/multer": "^1.4.12",
		"@types/node": "^20.10.7",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.6",
		"dotenv-cli": "^7.4.2",
		"nodemon": "^3.0.2",
		"sequelize-cli": "^6.6.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.5.4"
	}
}
